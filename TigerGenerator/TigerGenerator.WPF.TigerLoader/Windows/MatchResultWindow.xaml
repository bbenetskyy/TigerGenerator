<Window x:Class="TigerGenerator.WPF.TigerLoader.Forms.FightResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="FightResultWindow" Height="142.718" Width="335.534" Background="{StaticResource {x:Static SystemColors.InactiveCaptionBrushKey}}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <StackPanel x:Name="ElementsName"/>
        <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button x:Name="bSave" Margin="3">Save</Button>
            <Button x:Name="bCancel" Margin="3">Cancel</Button>
        </StackPanel>
    </Grid>
</Window>
