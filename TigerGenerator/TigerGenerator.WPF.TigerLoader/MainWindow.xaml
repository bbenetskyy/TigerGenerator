<Window x:Class="TigerGenerator.WPF.TigerLoader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TigerGenerator.WPF.TigerLoader"
        mc:Ignorable="d" 
        Title="MainWindow"  Height="275.641" Width="274.471" 
        WindowStyle="None" 
        Icon="Images/TigerLogo.ico" 
        WindowStartupLocation="CenterScreen" 
        ResizeMode='NoResize'
        AllowsTransparency="True"
        >
    <Window.Background>
        <LinearGradientBrush EndPoint="1,1" StartPoint="0.5,0">
            <GradientStop Color="Black"/>
            <GradientStop Color="#FFA60B0B" Offset="0.25"/>
            <GradientStop Color="Black" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid >
        <Image Source="Images/TigerLogo.png"  />
    </Grid>
</Window>
